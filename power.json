{
  "name": "trello",
  "displayName": "Trello",
  "version": "1.0.0",
  "description": "Manage Trello boards, lists, cards, and organizations directly from Kiro",
  "keywords": [
    "trello",
    "project management",
    "boards",
    "cards",
    "lists",
    "kanban",
    "task management",
    "organization",
    "workflow"
  ],
  "author": "cargom98",
  "repository": "https://github.com/cargom98/gm-trello-mcp",
  "mcp": {
    "trello": {
      "command": "uvx",
      "args": ["trello-mcp-server"],
      "env": {
        "TRELLO_API_KEY": "${TRELLO_API_KEY}"
      }
    }
  }
}
